<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:table='urn:import:za.ac.sun.cs.hons.minke.client.gui.table'>
	<g:DockLayoutPanel unit="EM" width="1000px" height="500px">
		<g:center>
			<table:ShoppingListDetail ui:field="listDetails" />
		</g:center>
		<g:south size="2">
			<g:SimplePanel>
				<g:Button ui:field='closeButton' text="Close" />
			</g:SimplePanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>
