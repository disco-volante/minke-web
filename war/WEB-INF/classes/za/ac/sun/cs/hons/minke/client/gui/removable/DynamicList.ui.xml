<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.header {
			height: 100%;
			width: 100%;
		}
		
		.header td {
			font: bold italic "Times New Roman", "MS Serif", "New York", serif;
			font-weight: bold;
			text-shadow: #fff 0 2px 2px;
			padding: 2px 0 1px 10px;
			color: #646572;
		}
	</ui:style>
			<g:ScrollPanel width="320px" height="200px" styleName="{style.header}">
				<g:VerticalPanel ui:field='itemList' styleName="{style.header}" />
			</g:ScrollPanel>
</ui:UiBinder>
