<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" >

    <g:FlowPanel>

        <g:HTML>

            <h3>
Select Product Timelines
            </h3>

            <br>
            </br>
        </g:HTML>

        <g:ListBox
            visibleItemCount="1"
            ui:field="graphList" >

            <g:item value="all" >
				All
            </g:item>

            <g:item value="selected" >
				Selected only.
            </g:item>

            <g:item value="same" >
				Selected and others like it.
            </g:item>
        </g:ListBox>

        <g:HorizontalPanel>

            <g:Button
                text="Get Timelines"
                ui:field="graphButton" />

            <g:Button
                text="Cancel"
                ui:field="cancelButton" />
        </g:HorizontalPanel>
    </g:FlowPanel>

</ui:UiBinder>