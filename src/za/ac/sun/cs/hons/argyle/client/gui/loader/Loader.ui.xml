<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field="images"
		type="za.ac.sun.cs.hons.argyle.client.util.ImageUtils.Images" />
	<ui:style>
		.transparent {
			opacity: 0.4;
		}
		
		.visible {
			opacity: 1.0;
		}
	</ui:style>
	<g:Image ui:field="loaderImg" resource="{images.loader}"
		styleName='{style.visible}' />
</ui:UiBinder>
