<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field="banner"
		type="za.ac.sun.cs.hons.argyle.client.util.ImageUtils.Images"/>
	<g:SimplePanel width="100%" height="100%">
			<g:Image ui:field="image" width="100%" height="100%"
				resource="{banner.banner}"/>
	</g:SimplePanel>
</ui:UiBinder>
