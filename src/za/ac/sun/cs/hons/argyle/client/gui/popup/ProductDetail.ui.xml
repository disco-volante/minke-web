<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:FlowPanel>
		<g:Label text="Product Details" horizontalAlignment="ALIGN_CENTER" />
		<g:HorizontalPanel width="155px">
			<g:Label text="Name" horizontalAlignment="ALIGN_LEFT" />
			<g:Label ui:field='name' horizontalAlignment="ALIGN_RIGHT" />
		</g:HorizontalPanel>
		<g:HorizontalPanel width="155px">
			<g:Label text="Store" horizontalAlignment="ALIGN_LEFT" />
			<g:Label ui:field='store' horizontalAlignment="ALIGN_RIGHT" />
		</g:HorizontalPanel>
		<g:HorizontalPanel width="155px">
			<g:Label text="Location" horizontalAlignment="ALIGN_LEFT" />
			<g:Label ui:field='location' horizontalAlignment="ALIGN_RIGHT" />
		</g:HorizontalPanel>
		<g:HorizontalPanel width="155px">
			<g:Label text="Date" horizontalAlignment="ALIGN_LEFT" />
			<g:Label ui:field='date' horizontalAlignment="ALIGN_RIGHT" />
		</g:HorizontalPanel>
		<g:HorizontalPanel width="155px">
			<g:Label text="Price" horizontalAlignment="ALIGN_LEFT" />
			<g:Label ui:field='price' horizontalAlignment="ALIGN_RIGHT" />
		</g:HorizontalPanel>
		<g:HorizontalPanel width="155px">
			<g:Label text="Size" horizontalAlignment="ALIGN_LEFT" />
			<g:Label ui:field='size' horizontalAlignment="ALIGN_RIGHT" />
		</g:HorizontalPanel>
	<g:Button ui:field='closeButton' text="Close" />
	</g:FlowPanel>
</ui:UiBinder>
