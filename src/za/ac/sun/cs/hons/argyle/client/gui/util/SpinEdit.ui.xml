<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:za.ac.sun.cs.hons.argyle.client.gui.button'>
	<g:DockLayoutPanel unit="EM">
		<g:west size="4">
			<g:IntegerBox ui:field='integerBox' value='1' width="16px" />
		</g:west>
		<g:center>
			<g:VerticalPanel>
				<b:UpButton ui:field='upButton' />
				<b:DownButton ui:field='downButton' />
			</g:VerticalPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
